@font-face {
	font-family: 'coaching';
	src:url('./fonts/coaching.eot?ftknts');
	src:url('./fonts/coaching.eot?#iefixftknts') format('embedded-opentype'),
		url('./fonts/coaching.ttf?ftknts') format('truetype'),
		url('./fonts/coaching.woff?ftknts') format('woff'),
		url('./fonts/coaching.svg?ftknts#coaching') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'coaching';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

